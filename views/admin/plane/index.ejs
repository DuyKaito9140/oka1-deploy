<!DOCTYPE html>
<html lang="en">
  <head>  
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
      integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh"
      crossorigin="anonymous"
    /> 
    
  </head>
  <body>
    <div style="width: 100%; height: 60px; background-color: #00a6ff;"></div>      
    <br /> 
    <div class="container mt-3">
      <a href="/plane/add" class="btn btn-success">Thêm mới máy bay</a>
      <table class="table">
        <thead>
          <th>ID máy bay</th>
          <th>Tên máy bay</th>
          <th>Hãng máy bay</th>
          <th>Số lượng khách</th>
          <th></th>
        </thead>
        <tbody>
          <% result.recordset.forEach(function (maybay) { %>
            <% if(maybay.TenHang == user2.data.partnerUsername) {%>
            <tr>  
                <td><%= maybay.idMayBay %></td>  
                <td><%= maybay.TenMayBay %></td>
                <td><%= maybay.TenHang %></td>
                <td><%= maybay.SoLuongKhach %></td>
                <td>
                  <a href="/plane/edit/<%= maybay.idMayBay %>" class="btn btn-primary">Sửa</a>
                  <a href="/plane/delete/<%= maybay.idMayBay %>" class="btn btn-danger">Xóa</a>
                </td>
            </tr>   
            <% } %>                                
         <% }) %>
        </tbody>
      </table>
    </div> 
  </body>
</html>
